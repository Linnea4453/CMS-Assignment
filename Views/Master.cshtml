@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
}

@* the fun starts here *@

<!DOCTYPE html>
<html lang="sv">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    
    <script src="https://kit.fontawesome.com/ca6ed7d2e3.js" crossorigin="anonymous"></script>
   <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/Mainstyle.css">
    <link rel="stylesheet" href="/css/Crewstyle.css">
     
    <title>Testar design</title>
</head>
<body>
    <div class="container">

        <header>
          <h2>Foods & Drinks </h2>
        </header>
        <section>
            <nav class="navbar navbar-expand-lg">
                <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
                  <div class="navbar-nav">
                      
                        <a class="p-2" href="@Umbraco.Content("e16b9190-80d8-4d3d-ab32-bcfa3ac914b6").Url()">Home</a>
                       <a class="p-2" href="@Umbraco.Content("988941cc-c308-4400-b8df-c79bd37251af").Url()">Our Crew</a>
                      <a class="p-2" href="@Umbraco.Content("62510d90-e632-4b34-adcc-e8c185b33d06").Url()">Contact Us</a>
                 
                        @{
                        	var selection = Umbraco.Content(1058)
                            .Children()
                            .Where(x => x.IsVisible())
                            .OrderBy(x => x.Name);
                        }
                        
                        	@foreach (var item in selection)
                        	{
                        			<a class="p-2" href="@item.Url()">@item.Name()</a>
                        	}
 
                       
                  </div>
                </div>
            </nav>
            <div>
                @Html.Partial("loginStatusPartial")
            </div>
        </section>
      
        
        <section>  
      
          <article class="articletext">
              <div  class="p-4">
                  @RenderBody()
              </div>
               
         
            
          </article>
          <nav2>
            <h4>Random Recept</h4>
           <p>@Html.Partial("RandomPostsPartial") </p> 
          </nav2>
        </section>
        
     <!--Start Footer-->
        <footer >
          <ul  >
              <li class="list-inline-item"><a href="#">Home</a></li>
              <li class="list-inline-item"><a href="#">Services</a></li>
              <li class="list-inline-item"><a href="#">About</a></li>
              <li class="list-inline-item"><a href="#">Contact us</a></li>
              <li class="list-inline-item"><a href="#">Private Policys</a></li>
          </ul>
          <div>
        <a href="#"><i class="fab fa-twitter"></i></a>
        <a href="#"><i class="fab fa-facebook"></i></a>
        <a href="#"><i class="fab fa-instagram"></i></a>
        <a href="#"><i class="fab fa-snapchat"></i></a>
          </div>
        <p>Foods&Drinks © 2021</p>
        </footer>
  <!--End Footer-->

    </div>
   
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta2/dist/js/bootstrap.bundle.min.js" integrity="sha384-b5kHyXgcpbZJO/tY9Ul7kGkf1S0CWuKcCD38l8YkeH8z8QjE0GmW1gYU5S9FOnJ0" crossorigin="anonymous"></script>
</body>
</html>